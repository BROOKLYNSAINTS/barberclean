{
  "cli": {
    "version": ">= 13.0.0"
  },
  "build": {
    "preview": {
      "developmentClient": false,
      "node": "20.19.4",
      "channel": "staging",
      "distribution": "store",
      "env": {
        "EXPO_PUBLIC_BACKEND_URL": "https://barber-backend-ten.vercel.app"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    },
    "production": {
      "developmentClient": false,
      "node": "20.19.4",
      "channel": "production",
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "cache": {
        "disabled": false
      }
    }
  },
  "submit": {
    "preview": {
      "android": {
        "serviceAccountKeyPath": "./google-play-service-account.json",
        "track": "internal",
        "releaseStatus": "draft"
      }
    },
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-play-service-account.json",
        "track": "internal",
        "releaseStatus": "draft"
      },
      "ios": {
        "appleId": "coach@brooklynsaints.com",
        "ascAppId": "6746369434",
        "appleTeamId": "42G5N5JFV7"
      }
    }
  }
}
